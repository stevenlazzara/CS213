<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RecieptController">
   <center>
      <AnchorPane prefHeight="530.0" prefWidth="350.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="output" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="396.0" prefWidth="320.0" />
            <HBox alignment="CENTER" layoutX="14.0" layoutY="457.0" prefHeight="82.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearButtonPressed" prefHeight="32.0" prefWidth="80.0" text="Clear" />
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPressed" prefHeight="32.0" prefWidth="79.0" text="Back" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
